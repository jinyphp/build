{
    "name": "jiny/build",
    "description": "static build",
    "keywords": [
        "jinyPHP",
        "framwork",
        "build"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "livewire/livewire": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Build\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Build\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Build\\JinyBuildServiceProvider"
            ]
        }
    }
}
